{
  "name": "Minimal Youtube",
  "version": "1.0",
  "description": "This extension allows you to view youtube videos with less distractions.",
  "permissions": ["activeTab", "declarativeContent", "storage", "browsingData", "clipboardWrite", "clipboardRead"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["background.js"]
    }
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
